<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lab7.FindClassController">
   <children>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="400.0">
         <children>
            <TableView fx:id="subjectList" editable="true" prefHeight="500.0" prefWidth="400.0">
              <columns>
                <TableColumn fx:id="className" prefWidth="200.0" text="Subject" />
                <TableColumn fx:id="capacity" prefWidth="200.0" text="Occupancy" />
              </columns>
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </TableView>
            <HBox prefHeight="25.0" prefWidth="100.0">
               <children>
                  <TextField fx:id="filterField" prefWidth="350.0">
                     <HBox.margin>
                        <Insets left="25.0" />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="400.0">
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onMouseClicked="#onBackButtonClick" prefHeight="27.0" prefWidth="110.0" text="Back" />
                  <Button fx:id="signUpButton" mnemonicParsing="false" onMouseClicked="#onSignUpButtonClick" prefHeight="27.0" prefWidth="110.0" text="Enroll" />
               </children>
            </HBox>
         </children></VBox>
   </children>
</AnchorPane>
